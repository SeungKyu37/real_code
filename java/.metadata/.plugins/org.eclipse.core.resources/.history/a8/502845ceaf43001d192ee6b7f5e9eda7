package humanpractice;

public class P134_E4 {

	public static void main(String[] args) {
		int num = (int)(Math.random()*6) + 1;
		int num2 = (int)(Math.random()*6) + 1;
		int total;
		
		while (num <= 6, num2 <= 6) {
			total = num + num2;
			System.out.printf("(%d,%d), num, num2");
			
			if (total == 5) {
				break;
			}
			
		}
	}

}
